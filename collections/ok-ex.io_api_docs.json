					"console.log(ts)",
					"pm.environment.set(\"timestamp\", ts);",
					"",
					"let paramsObject = {};",
					"",
					"const okex_api_secret = pm.environment.get(\"secret_key\");",
					"",
					"const parameters = pm.request.headers;",
					"",
					"console.log(pm.request, pm.request.method)",
					"parameters.map((param) => {",
					"    if (param.key != 'x-signature' && ",
					"        param.key != 'x-timestamp' && ",
					"        !is_empty(param.value) &&",
					"        !is_disabled(param.disabled)) {",
					"            paramsObject[param.key] = param.value;",
					"    }",
					"})",
					"        ",
					"if (okex_api_secret) {",
					"",
					"    let queryString = \"\";",
					"    let path = '/' + pm.request.url.path.join('/')",
					"",
					"",
					"    let signing_data;",
					"    ",
					"",
					"    if (pm.request.method == 'GET') {",
					"        queryString = pm.request.url.query.map((item) => {",
					"            return `${item.key}=${item.value}`;",
					"        }).join('&');",
					"        ",
					"        if (!is_empty(queryString)) {",
					"            path += '?' + queryString",
					"        }",
					"        signing_data = `${pm.request.method}\\n${path}\\n${ts}`",
					"        console.log(\"signing_data 1\", `${pm.request.method}\\n${path}\\n${ts}`)",
					"    }",
					"    if ((pm.request.method == 'POST' || pm.request.method == 'PUT')) {",
					"        console.log('body req =>', pm.request.body.raw)",
					"        signing_data = `${pm.request.method}\\n${path}\\n${ts}`",
					"        queryString = pm.request.body.raw",
					"        if (queryString && queryString != '{}') {",
					"            console.log(signing_data,queryString);",
					"            signing_data = `${signing_data}\\n${btoa(queryString)}`",
					"            console.log(\"btoa -> \", btoa(queryString))",
					"        }",
					"    }",
					"",
					"    console.log(\"signing_data\", signing_data)",
					"    const signature = CryptoJS.HmacSHA256(signing_data , okex_api_secret).toString()",
					"     console.log(\"signature\", signature)",
					"    pm.environment.set(\"signature\", signature);",
					"}",
					"",
					"",
					"function is_disabled(str) {",
					"    return str == true;",
					"}",
					"",
					"function is_empty(str) {",
					"    if (typeof str == 'undefined' ||",
					"        !str || ",
					"        str.length === 0 || ",
					"        str === \"\" ||",
					"        !/[^\\s]/.test(str) ||",
					"        /^\\s*$/.test(str) ||",
					"        str.replace(/\\s/g,\"\") === \"\")",
					"    {",
					"        return true;",
					"    }",
					"    else",
					"    {",
					"        return false;",
					"    }",
					"}"
				]
